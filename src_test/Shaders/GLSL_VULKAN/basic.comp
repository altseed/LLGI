#version 430

struct VS_INPUT{
  float value;
};

layout(set = 0, binding = 0, std140) uniform CB
{
    float offset;
} _23;

layout(set = 0, binding = 1, std430) buffer ComputeBuffer
{
    VS_INPUT real[];
};

void main()
{
  uvec3 dtid = gl_GlobalInvocationID;
  real[dtid.x].value = real[dtid.x].value * real[dtid.x].value + _23.offset;
}